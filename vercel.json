{
    "version": 2,
    "functions": {
        "server/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "client/dist/$1"
        }
    ]
}